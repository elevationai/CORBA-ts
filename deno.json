{
  "name": "@eai/corba",
  "version": "1.0.0",
  "license": "MIT",
  "exports": "./mod.ts",
  "publish": {
    "include": [
      "mod.ts",
      "src/**/*.ts",
      "README.md",
      "LICENSE"
    ],
    "exclude": [
      "tests/**/*",
      "examples/**/*",
      "specs/**/*",
      "*.test.ts",
      "*_test.ts"
    ]
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": false,
    "proseWrap": "preserve"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "tasks": {
    "test": "deno test",
    "lint": "deno lint",
    "fmt": "deno fmt"
  }
}
